{
  "host_env_var_prefix": "SSO",
  "file_path": "./docker/directory-sso/.env",
  "env_vars": {
    "required": [
      "PORT",
      "SECRET_KEY",
      "DATABASE_URL",
      "API_SIGNATURE_SECRET",
      "SESSION_COOKIE_DOMAIN",
      "SSO_SESSION_COOKIE",
      "EMAIL_HOST",
      "EMAIL_PORT",
      "EMAIL_HOST_USER",
      "EMAIL_HOST_PASSWORD",
      "DEFAULT_FROM_EMAIL",
      "LOGOUT_REDIRECT_URL",
      "REDIRECT_FIELD_NAME",
      "ALLOWED_REDIRECT_DOMAINS",
      "PROXY_SIGNATURE_SECRET"
    ],
    "optional": [
      "DEBUG",
      "SSO_SESSION_COOKIE_SECURE",
      "DEFAULT_REDIRECT_URL"
    ]
  }
}

